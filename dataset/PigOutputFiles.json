{
	"name": "PigOutputFiles",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage_LinkedService",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": "*",
			"folderPath": "sm-pig-output/address"
		}
	}
}